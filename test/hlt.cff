// HLT filter modules

// Single photon
	module hlt1g = HLT1Photon {
		InputTag inputTag = simrec:photons
		double MinPt = 80.0
		double MaxEta= 2.4
		int32  MinN  = 1
	}

// Double photon
	module hlt2g1 = HLT1Photon {
		InputTag inputTag = simrec:photons
		double MinPt = 30.0
		double MaxEta= 2.4
		int32  MinN  = 1
	}
	module hlt2g2 = HLT1Photon {
		InputTag inputTag = simrec:photons
		double MinPt = 20.0
		double MaxEta= 2.4
		int32  MinN  = 2
	}

// Single electron
	module hlt1e = HLT1Electron {
		InputTag inputTag = simrec:electrons
		double MinPt = 26.0
		double MaxEta= 2.4
		int32  MinN  = 1
	}

// Double electron
	module hlt2e = HLT1Electron {
		InputTag inputTag = simrec:electrons
		double MinPt = 12.0
		double MaxEta= 2.4
		int32  MinN  = 2
	}

// Single muon
	module hlt1m = HLT1Muon {
		InputTag inputTag = simrec:muons
		double MinPt = 19.0
		double MaxEta= 2.0
		int32  MinN  = 1
	}

// Double muon
	module hlt2m = HLT1Muon {
		InputTag inputTag = simrec:muons
		double MinPt = 7.0
		double MaxEta= 2.0
		int32  MinN  = 2
	}

// Single tau
	module hlt1t = HLT1Tau  {
		InputTag inputTag = simrec:taus
		double MinPt = 100.0
		double MaxEta= 2.4
		int32  MinN  = 1
	}

// Double tau
	module hlt2t = HLT1Tau  {
		InputTag inputTag = simrec:taus
		double MinPt = 60.0
		double MaxEta= 2.4
		int32  MinN  = 2
	}

// Single jet
	module hlt1j = HLT1CaloJet {
		InputTag inputTag = simrec:jets
		double MinPt = 400.0
		double MaxEta= 5.0
		int32  MinN  = 1
	}

// Double jet
	module hlt2j = HLT1CaloJet {
		InputTag inputTag = simrec:jets
		double MinPt = 350.0
		double MaxEta= 5.0
		int32  MinN  = 2
	}

// Triple jet
	module hlt3j = HLT1CaloJet {
		InputTag inputTag = simrec:jets
		double MinPt = 195.0
		double MaxEta= 5.0
		int32  MinN  = 3
	}

// Quadruple jet
	module hlt4j = HLT1CaloJet {
		InputTag inputTag = simrec:jets
		double MinPt = 80.0
		double MaxEta= 5.0
		int32  MinN  = 4
	}


// Actual HLT trigger paths

	path p1g  = {doSimRec, hlt1g}
	path p2g  = {doSimRec, hlt2g1, hlt2g2}
	path p1e  = {doSimRec, hlt1e}
	path p2e  = {doSimRec, hlt2e}
	path p1m  = {doSimRec, hlt1m}
	path p2m  = {doSimRec, hlt2m}
	path p1t  = {doSimRec, hlt1t}
	path p2t  = {doSimRec, hlt2t}
	path p1j  = {doSimRec, hlt1j}
	path p2j  = {doSimRec, hlt2j}
	path p3j  = {doSimRec, hlt3j}
	path p4j  = {doSimRec, hlt4j}


// Tree of HLT filter objects

	module hlt = HLTMakeSummaryObjects { }
	endpath endhlt = {hlt}

// Want trigger table summary in log file & trigger results object

	untracked PSet options = {
		untracked bool wantSummary = true  # default is false
		untracked bool makeTriggerResults = true 
//		vstring listOfTriggers = {"p1","p2","p3","p4","p5"}
	}
